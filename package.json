{
  "name": "@sensate/sensate",
  "version": "0.0.2",
  "description": "An extensible bot framework that makes sense.",
  "main": "lib/index.js",
  "scripts": {
    "build": "make build",
    "test": "make test",
    "coverage": "make test-cov",
    "coverage-level": "make test-cov-level",
    "lint": "make lint",
    "flow": "make flow",
    "postpublish": "greenkeeper-postpublish",
    "prepublish": "npm run build"
  },
  "repository": "sensate-framework/sensate",
  "bugs": "https://github.com/sensate-framework/sensate/issues",
  "homepage": "https://github.com/sensate-framework/sensate#readme",
  "author": "Nico Hämäläinen <nico@sizeof.io>",
  "keywords": [
    "bot",
    "chatbot",
    "framework"
  ],
  "license": "MIT",
  "pre-commit": {
    "silent": true,
    "run": [
      "lint",
      "flow",
      "test",
      "coverage"
    ]
  },
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "cache": true,
    "check-coverage": true
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "mocha": true
    },
    "plugins": [
      "flowtype"
    ]
  },
  "dependencies": {
    "es6-enum": "^1.0.3",
    "eventemitter3": "^1.2.0",
    "request": "^2.74.0",
    "request-promise-native": "^1.0.3",
    "winston": "^2.2.0",
    "ws": "^2.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint-plugin-flowtype": "^2.7.1",
    "flow-bin": "^0.30.0",
    "greenkeeper-postpublish": "^1.0.1",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.0.0-beta.5"
  }
}
